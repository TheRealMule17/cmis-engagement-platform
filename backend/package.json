{
    "name": "cmis-events-lambdas",
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "test": "jest",
        "lint": "eslint src/",
        "build": "npm run clean && npm run zip",
        "clean": "rimraf dist && mkdir dist",
        "zip:processWaitlist": "powershell Compress-Archive -Force -Path src/lambdas/processWaitlist.js,node_modules -DestinationPath dist/processWaitlist.zip",
        "zip:archivePastEvents": "powershell Compress-Archive -Force -Path src/lambdas/archivePastEvents.js,node_modules -DestinationPath dist/archivePastEvents.zip",
        "zip": "npm run zip:processWaitlist && npm run zip:archivePastEvents"
    },
    "dependencies": {
        "@aws-sdk/client-dynamodb": "^3.500.0",
        "@aws-sdk/lib-dynamodb": "^3.500.0",
        "uuid": "^9.0.0"
    },
    "devDependencies": {
        "@types/node": "^25.2.3",
        "@types/uuid": "^10.0.0",
        "eslint": "^8.0.0",
        "jest": "^29.0.0",
        "rimraf": "^6.1.3"
    }
}
